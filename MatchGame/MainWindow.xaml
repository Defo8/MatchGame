        mc:Ignorable="d"
        Title="Znajdź wszystkie pary pasujących zwierząt" Height="450" Width="400">
    <Grid x:Name="mainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBlock Text="?" Grid.Column="0" Grid.Row="0" RenderTransformOrigin="2.009,3.25" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36"/>
        <TextBlock Text="?" Grid.Column="1" Grid.Row="0" RenderTransformOrigin="2.009,3.25" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36"/>
        <TextBlock Text="?" Grid.Column="2" Grid.Row="0" RenderTransformOrigin="2.009,3.25" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36"/>
        <TextBlock Text="?" Grid.Column="3" Grid.Row="0" RenderTransformOrigin="2.009,3.25" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36"/>
